<FetaOptions>
    <DataFile>
        <GraphOutput>experiments/netsci18/PFPgrown.dat</GraphOutput>
        <GraphInput>scripts/simple_start_graph.dat</GraphInput>
        <FormatRead>NNT</FormatRead>
        <Complex>false</Complex>
    </DataFile>
    <Action>
        <Type>Grow</Type>
        <Start>10</Start>
    </Action>
    <OperationModel>
        <Cliques>false</Cliques>
        <Type>feta.operationmodel.PreferentialAttachment</Type>
        <MaxNodes>100000</MaxNodes>
        <AddNodes>3</AddNodes>
    </OperationModel>
    <ObjectModel>
        <Entity>Link</Entity>
        <Multiply>false</Multiply>
        <Element>
            <Type>feta.objectmodel.DegreeModelElement</Type>
            <Weight>1.0</Weight>
        </Element>
    </ObjectModel>
    <ObjectModel>
        <Entity>Node</Entity>
        <LazyNormalise>false</LazyNormalise>
        <Multiply>false</Multiply>
        <Element>
            <Type>feta.objectmodel.PFPModelElement</Type>
            <Weight>1.0</Weight>
            <d>0.048</d>
        </Element>
    </ObjectModel>
</FetaOptions>
