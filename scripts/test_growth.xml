<FetaOptions>
    <DataFile>
        <GraphOutput>test.dat</GraphOutput>
        <GraphInput>scripts/simple_start_graph.dat</GraphInput>
        <FormatRead>NNT</FormatRead>
        <Complex>false</Complex>
    </DataFile>
    <Action>
        <Type>Grow</Type>  
        <Start>10</Start>
    </Action>
    <OperationModel>
        <Cliques>false</Cliques>
        <Type>feta.operationmodel.PreferentialAttachment</Type>
        <MaxNodes>100000</MaxNodes>
    </OperationModel>
    <ObjectModel>
        <Entity>Link</Entity>
        <Multiply>false</Multiply>
        <Element>
            <Type>feta.objectmodel.DegreeModelElement</Type>
            <Weight>2.0</Weight>
        </Element>
        <Element>
            <Type>feta.objectmodel.RandomModelElement</Type>
            <Weight>1.4</Weight>
        </Element>
    </ObjectModel>
    <ObjectModel>
        <Entity>Node</Entity>
        <Multiply>false</Multiply>
        <Element>
            <Type>feta.objectmodel.DegreePowModelElement</Type>
            <Power>1.5</Power>
            <Weight>0.65</Weight>
        </Element>
        <Element>
            <Type>feta.objectmodel.DegreeModelElement</Type>
            <Weight>1.4</Weight>
        </Element>
    </ObjectModel>
</FetaOptions>
